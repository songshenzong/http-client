{
  "name": "songshenzong/http-client",
  "description": "extension PHP HTTP client for Guzzle",
  "keywords": [
    "HTTP client",
    "RESTful",
    "Guzzle",
    "Laravel",
    "notification"
  ],
  "support": {
    "issues": "https://github.com/songshenzong/http-client/issues",
    "source": "https://github.com/songshenzong/http-client"
  },
  "type": "library",
  "license": "Apache-2.0",
  "homepage": "https://songshenzong.com",
  "authors": [
    {
      "name": "Songshenzong",
      "email": "i@songshenzong.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "^6.3",
    "songshenzong/support": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "laravel/framework": "5.6.*"
  },
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Songshenzong\\HttpClient\\": "src",
      "Songshenzong\\HttpClient\\Test\\": "tests"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Songshenzong\\HttpClient\\ServiceProvider"
      ]
    }
  }
}
